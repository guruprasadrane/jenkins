node {
  // Checkout the source code
  checkout scm

  // Remove the substring from the string using .split()
  def fullpath = "env/alpha"
  def rootpath = "env/"
  def result = fullpath.split(rootpath)

  // Print the result
  echo result

  // Remove any whitespace from the result using .trim()
  def trimmed = result.trim()

  // Print the trimmed result
  echo trimmed
}